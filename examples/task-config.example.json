 {
  "name": "mytask",
  "description": "My task",
  "entryPoint": "build/index",
  "runtime": "nodejs14.x",
  "path": "/path/here",
  "timeLimit": 180,
  "memoryLimit": 512,
  "environment": {
    "myvariable": "myvalue",
    "myenvironmentvariable": "$ENV_VALUE"
  },
  "executionPermission": "permissionRequired",
  "retryPolicy": {
    "enabled": true,
    "errorsToRetry": [ "Test error", "Another error"]
  }
}